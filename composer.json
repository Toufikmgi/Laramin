{
    "name": "simoja/laramin",
    "description": "Small Laravel Admin",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed Benhida",
            "email": "mohameedbenhida@gmail.com"
        }
    ],
    "require": {
          "santigarcor/laratrust": "4.0.*"
    },
     "autoload" : {
        "psr-4": {
            "Simoja\\Laramin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Simoja\\Laramin\\Tests\\": "tests/"
      }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Simoja\\Laramin\\LaraminServiceProvider"
            ],
             "aliases": {
                "Laramin": "Simoja\\Laramin\\Facades\\Laramin"
            }
        }
}
}
